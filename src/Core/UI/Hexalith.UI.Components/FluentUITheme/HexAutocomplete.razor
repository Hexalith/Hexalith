@inherits FluentAutocomplete<Option<string>>

<FluentAutocomplete TOption="Option<string>"
                    AdditionalAttributes="AdditionalAttributes"
                    Appearance="Appearance"
                    AriaLabel="@AriaLabel"
                    AutoComplete="@AutoComplete"
                    Autofocus="@Autofocus"
                    Class="@Class"
                    Context="Context"
                    Data="@Data"
                    Disabled="Disabled"
                    DisplayName="@DisplayName"
                    Embedded="Embedded"
                    Field="Field"
                    Height="@Height"
                    IconDismiss="IconDismiss"
                    IconSearch="IconSearch"
                    Id="@Id"
                    Immediate="Immediate"
                    ImmediateDelay="ImmediateDelay"
                    Items="Items"
                    ItemSize="ItemSize"
                    KeepOpen="KeepOpen"
                    Label="@Label"
                    MaxAutoHeight="@MaxAutoHeight"
                    MaximumOptionsSearch="MaximumOptionsSearch"
                    MaximumSelectedOptions="1"
                    Multiple="true"
                    Name="@Name"
                    OptionClass="@OptionClass"
                    OptionDisabled="@(item => item.Disabled)"
                    OptionSelected="@(item => item.Selected)"
                    OptionStyle="@OptionStyle"
                    OptionText="@(item => item.Text)"
                    OptionValue="@(item => item.Value)"
                    OnOptionsSearch="OnOptionsSearch"
                    ParentReference="ParentReference"
                    Placeholder="@Placeholder"
                    ReadOnly="ReadOnly"
                    Required="Required"
                    SelectedOption="SelectedOption"
                    SelectedOptionChanged="SelectedOptionChanged"
                    SelectedOptions="SelectedOptions"
                    SelectedOptionsChanged="SelectedOptionsChanged"
                    SelectValueOnTab="SelectValueOnTab"
                    ShowOverlayOnEmptyResults="ShowOverlayOnEmptyResults"
                    Style="@Style"
                    TitleScrollToNext="@TitleScrollToNext"
                    TitleScrollToPrevious="@TitleScrollToPrevious"
                    ValueExpression="ValueExpression"
                    ValueText="ValueText"
                    ValueTextChanged="ValueTextChanged"
                    Virtualize="Virtualize"
                    Width="@(Width ?? "100%")">
    <MaximumSelectedOptionsMessage>
        @MaximumSelectedOptionsMessage
    </MaximumSelectedOptionsMessage>
    <OptionTemplate>
        @OptionTemplate
    </OptionTemplate>
    <SelectedOptionTemplate>
        @SelectedOptionTemplate
    </SelectedOptionTemplate>
    <LabelTemplate>
        @LabelTemplate
    </LabelTemplate>
    <HeaderContent>
        @HeaderContent
    </HeaderContent>
    <ChildContent>
        @ChildContent
    </ChildContent>
    <FooterContent>
        @FooterContent
    </FooterContent>
</FluentAutocomplete>

