@inherits FluentTimePicker

<FluentTimePicker AdditionalAttributes="AdditionalAttributes"
                  Appearance="@Appearance"
                  AriaLabel="@AriaLabel"
                  Autofocus="@Autofocus"
                  Class="@Class"
                  Data="@Data"
                  Disabled="@Disabled"
                  DisplayName="@DisplayName"
                  Embedded="@Embedded"
                  Field="@Field"
                  Id="@Id"
                  Immediate="@Immediate"
                  ImmediateDelay="@ImmediateDelay"
                  Label="@(Label ?? Labels.StartTime)"
                  Name="@Name"
                  ParentReference="@ParentReference"
                  Placeholder="@Placeholder"
                  ReadOnly="@ReadOnly"
                  Required="true"
                  Style="@(Style ?? "width:100%;")"
                  Value="@Value"
                  ValueChanged="@ValueChanged"                  
                  ValueExpression="@ValueExpression">
    <LabelTemplate>
        @LabelTemplate
    </LabelTemplate>
</FluentTimePicker>
